<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    tools:ignore="NewApi">
    <item
        android:id="@+id/checked"
        android:state_checked="true"
        android:drawable="@drawable/sfb_check_box_checked_witout_tick"
        />
    <item
        android:id="@+id/unchecked"
        android:drawable="@drawable/sfb_check_box_uncheck_without_tick"/>
    <transition
        android:fromId="@id/unchecked"
        android:toId="@id/checked"
        android:drawable="@drawable/sfb_avd_unchek_to_check"
        />
    <transition
        android:fromId="@id/checked"
        android:toId="@id/unchecked"
        android:drawable="@drawable/sfb_avd_check_to_uncheck2"
        />
</animated-selector>